<?xml version="1.0" encoding="utf-8"?>
<installer-gui-script minSpecVersion="2">
    <title>Thunder Compute CLI (tnr)</title>
    <options customize="never" require-scripts="false" hostArchitectures="x86_64,arm64"/>
    <domains enable_anywhere="false" enable_currentUserHome="false" enable_localSystem="true"/>
    <choices-outline>
        <line choice="default">
            <line choice="com.thundercompute.tnr"/>
        </line>
    </choices-outline>
    <choice id="default"/>
    <choice id="com.thundercompute.tnr" visible="false">
        <pkg-ref id="com.thundercompute.tnr"/>
    </choice>
    <pkg-ref id="com.thundercompute.tnr" version="0" onConclusion="none">component.pkg</pkg-ref>
    <conclusion file="conclusion.html" mime-type="text/html"/>
</installer-gui-script>
